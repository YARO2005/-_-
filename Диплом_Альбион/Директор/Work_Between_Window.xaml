<Window x:Class="Диплом_Альбион.Директор.Work_Between_Window"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:Диплом_Альбион.Директор"
        mc:Ignorable="d"
        Title="Промежуточное окно" Height="180" Width="300"
        ResizeMode="NoResize">
    <Grid>
        <Button x:Name="Button_To_Manager" Content="Войти как менеджер" HorizontalAlignment="Center" Margin="0,51,0,0" VerticalAlignment="Top" Width="140" Click="Button_To_Manager_Click"/>
        <Button x:Name="Button_To_Director" Content="Войти как директор" HorizontalAlignment="Center" VerticalAlignment="Top" Width="140" Margin="0,83,0,0" Click="Button_To_Director_Click"/>
        <Button x:Name="Button_Exite" Content="Выход" HorizontalAlignment="Center" Margin="0,115,0,0" VerticalAlignment="Top" Width="56" Click="Button_Exite_Click"/>
        <Label Content="Выберите рабочее окно" HorizontalAlignment="Center" Margin="0,9,0,0" VerticalAlignment="Top"/>

    </Grid>
</Window>
